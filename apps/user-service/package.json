{
  "name": "@eats/user",
  "version": "0.0.1",
  "description": "apis for auth service",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "12.x || 14.x",
    "npm": ">=6.9.x"
  },
  "devEngines": {
    "node": "12.x || 14.x",
    "npm": ">=6.9.x"
  },
  "scripts": {
    "start": "nest start",
    "prestart": "npm run migration:run",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "tsc -p tsconfig.build.json",
    "check-engines": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "prebuild": "npm run check-engines && npm run clean",
    "build:prod": "npm run build",
    "clean": "rimraf dist/src/",
    "test": "npm run test:migration && npm run test:ci && npm run test:e2e",
    "test:unit": "jest",
    "test:migration": "dotenv -e env.test --  typeorm migration:run -t each",
    "test:ci": "NODE_ENV=CI jest --ci none --testPathPattern='src' --forceExit true --collectCoverage --maxWorkers 2 --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest.config-e2e.js --runInBand --silent --detectOpenHandles",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.ts",
    "migration:generate": "typeorm migration:create -n",
    "migration:run": "typeorm migration:run -t each",
    "migration:revert": "typeorm migration:revert",
    "db:sync": "npm run typeorm:cli schema:sync"
  },
  "dependencies": {
    "@nanogiants/nestjs-swagger-api-exception-decorator": "1.5.0",
    "@nestjs/common": "^8.0.11",
    "@nestjs/core": "^8.0.11",
    "@nestjs/event-emitter": "1.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@eats/config": "1.0.3",
    "@eats/logger": "1.0.0",
    "@eats/database": "*",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.1.0",
    "@nestjs/terminus": "^8.0.1",
    "@nestjs/typeorm": "^8.0.2",
    "@types/uuid": "^9.0.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "2.2.2",
    "moment": "^2.29.1",
    "parse-domain": "4.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.38",
    "url-join-ts": "^1.0.5",
    "uuid": "^9.0.0",
    "uuidv4": "^6.2.12",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "concurrently": "^5.0.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "mock-jwks": "^1.0.1",
    "nock": "^13.1.3",
    "nodemon": "^2.0.13",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
